var app = sm('do_App');
var viewShower = ui('do_ViewShower_1');
var pages = [
       {id:'basket',path:'source://view/basket.ui'}
       ];

ui('do_Button_1').on('touch',function(){
	viewShower.addViews(pages);
	viewShower.showView('basket');
})
page.on('loaded')
